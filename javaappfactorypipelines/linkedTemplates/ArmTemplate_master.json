{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "javaappfactorypipelines"
		},
		"LS_ADLSGen2_Sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccdataloadtest.dfs.core.windows.net"
		},
		"LS_Azure_Key_Vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-data-engine-secret.vault.azure.net/"
		},
		"LS_Azure_SQL_Source_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnectionstring"
		},
		"LS_Project_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-secrets-data.vault.azure.net/"
		},
		"LS_Project_Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccdataloadtest.dfs.core.windows.net"
		},
		"LS_Project_Azure_SQL_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnectionString"
		},
		"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelinefoldername": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipeinefilename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelineschemaname": {
			"type": "string",
			"defaultValue": "dbo"
		},
		"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelinetablename": {
			"type": "string",
			"defaultValue": "@triggerBody().tableName"
		},
		"EventBasedTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/97b7e41b-5696-4069-a341-130144932995/resourceGroups/testrg/providers/Microsoft.Storage/storageAccounts/storageaccdataloadtest"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "javaappfactorypipelines_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Azure_Key_Vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_Azure_Key_Vault_properties_typeProperties_baseUrl')]"
					},
					"LS_Project_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_Project_kv_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLSGen2_Sink_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSGen2_Sink_properties_typeProperties_url')]"
					},
					"LS_Azure_SQL_Source_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_Azure_SQL_Source_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_Project_Gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_Project_Gen2_properties_typeProperties_url')]"
					},
					"LS_Project_Azure_SQL_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_Project_Azure_SQL_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "javaappfactorypipelines_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelinefoldername": {
						"value": "[parameters('EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelinefoldername')]"
					},
					"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipeinefilename": {
						"value": "[parameters('EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipeinefilename')]"
					},
					"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelineschemaname": {
						"value": "[parameters('EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelineschemaname')]"
					},
					"EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelinetablename": {
						"value": "[parameters('EventBasedTrigger_properties_Event_Based_Trigger_parameters_pipelinetablename')]"
					},
					"EventBasedTrigger_properties_typeProperties_scope": {
						"value": "[parameters('EventBasedTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'javaappfactorypipelines_ArmTemplate_0')]"
			]
		}
	]
}