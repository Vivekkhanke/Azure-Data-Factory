			"defaultValue": "javaappfactorypipelines"
		},
		"LS_ADLSGen2_Sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccdataloadtest.dfs.core.windows.net"
		},
		"LS_Azure_Key_Vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-data-engine-secret.vault.azure.net/"
		},
		"LS_Azure_SQL_Source_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnectionstring"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
